Modelo6<-glance(Filt6) %>% dplyr::select(r.squared,df,AIC) %>% mutate(Modelo="Filt6")
Modelo7<-glance(Filt7) %>% dplyr::select(r.squared,df,AIC) %>% mutate(Modelo="Filt7")
Modelo8<-glance(Filt8) %>% dplyr::select(r.squared,df,AIC) %>% mutate(Modelo="Filt8")
Modelo9<-glance(Filt9) %>% dplyr::select(r.squared,df,AIC) %>% mutate(Modelo="Filt9")
Modelos<-bind_rows(Modelo1,Modelo2,Modelo3,Modelo4,Modelo5,Modelo6,Modelo7,Modelo8,Modelo9) %>% arrange(AIC)
