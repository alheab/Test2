Filt2<-lm(uptake~Type+conc, data = CO2)
###Pendiente, Distinta pendiente,pero mismo intercepto
Filt3<-lm(uptake~Type:conc, data = CO2)
###Interactivo,Distinta pendiente, distinto intercepto
Filt4<-lm(uptake~Type*conc, data = CO2)
Filt5<-lm(uptake~Type+conc+Treatment,data = CO2)
Filt6<-lm(uptake~Type*conc*Treatment,data = CO2)
Filt7<-lm(uptake~Type+I(log(conc))+Treatment,data = CO2)
Filt8<-lm(uptake~Type:I(log(conc))+Treatment,data = CO2)
Filt9<-lm(uptake~Type*I(log(conc))*Treatment,data = CO2)
Modelo1<-glance(Filt1) %>% dplyr::select(r.squared,df,AIC) %>% mutate(Modelo="Filt1")
Modelo2<-glance(Filt2) %>% dplyr::select(r.squared,df,AIC) %>% mutate(Modelo="Filt2")
